{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "server/**/*.js",
      "src/**/*.js",
      "src/**/*.jsx"
    ]
  },
  "rules": {
    "ordered-imports": false,
    "interface-name": [true, "never-prefix"],
    "object-literal-sort-keys": false,
    "no-console": false,
    "interface-over-type-literal": false,
    "jsx-no-lambda": false,
    "no-reference": false
  }
}
