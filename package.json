{
  "name": "danielak-presentes",
  "version": "0.1.0",
  "private": true,
  "proxy": "http://localhost:9000",
  "engines": {
    "node": ">=12.x",
    "yarn": "1.x"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "aws-sdk": "2.758.0",
    "axios": "0.20.0",
    "body-parser": "1.19.0",
    "classnames": "^2.2.6",
    "dotenv": "^8.2.0",
    "express": "4.17.1",
    "express-jwt": "6.0.0",
    "file-type": "^9.0.0",
    "final-form": "^4.20.1",
    "history": "^4.6.3",
    "jsonwebtoken": "8.5.1",
    "knex": "^0.21.5",
    "moment": "2.29.0",
    "multiparty": "^4.2.2",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pg": "^8.3.3",
    "pg-connection-string": "^2.3.0",
    "query-string": "6.13.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-dropzone": "^4.2.8",
    "react-final-form": "^6.5.1",
    "react-flexbox-grid": "^2.1.2",
    "react-ga": "3.1.2",
    "react-quill": "1.3.5",
    "react-responsive-carousel": "3.2.10",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "semantic-ui-css": "2.4.1",
    "semantic-ui-react": "^0.82.1",
    "sharp": "^0.25.4",
    "styled-components": "^5.2.0",
    "swr": "0.3.4",
    "use-query-params": "1.1.8"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/classnames": "2.2.10",
    "@types/express": "^4.17.8",
    "@types/express-jwt": "^0.0.42",
    "@types/file-type": "^10.9.1",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multiparty": "^0.0.32",
    "@types/node": "^10.5.4",
    "@types/passport": "^1.0.4",
    "@types/passport-local": "^1.0.33",
    "@types/query-string": "6.3.0",
    "@types/react": "16.9.49",
    "@types/react-dom": "16.9.8",
    "@types/react-dropzone": "^4.2.1",
    "@types/react-router-dom": "^5.1.5",
    "@types/sharp": "^0.26.0",
    "@types/styled-components": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "@typescript-eslint/parser": "4.2.0",
    "eslint": "7.9.0",
    "eslint-config-airbnb-typescript": "10.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "7.21.0",
    "eslint-plugin-react-hooks": "4.1.2",
    "eslint-watch": "^7.0.0",
    "lint-staged": "10.4.0",
    "node-sass": "4.14.1",
    "nodemon": "^2.0.4",
    "prettier": "2.1.2",
    "ts-node": "^9.0.0",
    "typescript": "4.0.3"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "start-client": "react-scripts start",
    "start-server": "APP_ENV=development nodemon -r dotenv/config --exec ts-node pg-server/index.ts",
    "build-client": "react-scripts build",
    "build-server": "tsc --project ./tsconfig.build.json",
    "heroku-postbuild": "ts-node ./scripts/build.ts",
    "test-client": "react-scripts test",
    "ts": "tsc --project ./tsconfig.json --noEmit",
    "ts:watch": "tsc --project ./tsconfig.json --watch --noEmit",
    "lint": "eslint ./src ./pg-server ./scripts -c .eslintrc --ext .js,.ts,.jsx,.tsx",
    "lint:watch": "esw ./src ./pg-server ./scripts -c .eslintrc --ext .js,.ts,.jsx,.tsx --watch --cache --color",
    "serve": "node -r dotenv/config build-server/index.js",
    "migrate:make": "knex --knexfile ./pg-server/knexfile.ts migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./pg-server/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./pg-server/knexfile.ts migrate:rollback",
    "create:db:dev": "APP_ENV=development ts-node ./scripts/createDb.ts"
  }
}
