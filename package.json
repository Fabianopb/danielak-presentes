    "tslint": "^5.18.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.5.2"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx}": [
      "eslint --fix"
    ]
  },
  "scripts": {
    "start-client": "react-scripts start",
    "start-server": "APP_ENV=development nodemon -r dotenv/config --exec ts-node pg-server/index.ts",
    "build-client": "react-scripts build",
    "build-server": "tsc --project ./tsconfig.build.json",
    "heroku-postbuild": "ts-node ./scripts/build.ts",
    "test-client": "react-scripts test",
    "ts": "tsc --project ./tsconfig.json --noEmit",
    "ts:watch": "tsc --project ./tsconfig.json --watch --noEmit",
    "lint": "eslint ./src ./pg-server ./scripts -c .eslintrc --ext .js,.ts,.jsx,.tsx",
    "lint:watch": "esw ./src ./pg-server ./scripts -c .eslintrc --ext .js,.ts,.jsx,.tsx --watch --cache --color",
    "serve": "node -r dotenv/config build-server/index.js",
    "migrate:make": "knex --knexfile ./pg-server/knexfile.ts migrate:make -x ts",
    "migrate:latest": "knex --knexfile ./pg-server/knexfile.ts migrate:latest",
    "migrate:rollback": "knex --knexfile ./pg-server/knexfile.ts migrate:rollback",
    "create:db:dev": "APP_ENV=development ts-node ./scripts/createDb.ts"
  }
}
